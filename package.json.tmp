{
  "name": "onback-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:backend": "env-cmd -e dev npm run dev -w apps/backend",
    "dev:frontend": "env-cmd -e dev npm run dev -w apps/frontend",
    "build:backend": "npm run build -w apps/backend",
    "build:frontend": "npm run build -w apps/frontend",
    "install:all": "npm install"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}
